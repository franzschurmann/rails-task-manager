<%= form_with model: task do |f| %>
  <%= f.label "Task Title" %>
  <%= f.text_field :title, placeholder: "Do nothing..." %>
  <%= f.label :details %>
  <%= f.text_field :details, placeholder: "put details" %>
  <%= f.label :completed? %>
  <%= f.text_field :completed, placeholder: "true" %>
  <%= f.submit %>
<% end %>
